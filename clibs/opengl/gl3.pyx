DEFAULT_CLEAR_MASK = c_gl3.GL_COLOR_BUFFER_BIT | c_gl3.GL_DEPTH_BUFFER_BIT

cdef getdefault(obj, key, default=None):
    try:
        return obj[key]
    except (KeyError, IndexError):
        return default

cpdef clear(color, depth=1.0):
    c_gl3.glClearColor(color[0], color[1], color[2], getdefault(color, 3, 1.0))
    c_gl3.glClearDepth(depth)
    c_gl3.glClear(DEFAULT_CLEAR_MASK)

